{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/tweet.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/tweet",
      "dest": "/api/tweet.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    }
  ],
  "crons": [
    {
      "path": "/api/tweet",
      "schedule": "0 12 * * *"
    }
  ]
}